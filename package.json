    "helmet": "^3.14.0",
    "jsonapi-serializer": "^3.6.4",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "morgan": "~1.9.0",
    "multer": "^1.3.1",
    "nodemailer": "^5.1.1",
    "nodemailer-mailgun-transport": "^1.4.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1"

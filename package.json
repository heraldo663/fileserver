{
  "name": "fileserver",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev:server": "NODE_ENV=development nodemon ./bin/www",
    "dev:client": "npm run serve --prefix client",
    "build:client": "npm run build --prefix client",
    "install:client": "npm install --prefix client",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watchAll --verbose --detectOpenHandles",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "helmet": "^3.14.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "morgan": "~1.9.0",
    "multer": "^1.3.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "jest-environment-node": "^23.4.0",
    "nodemon": "^1.18.4",
    "supertest": "^3.3.0"
  }
}
